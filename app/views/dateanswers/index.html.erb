<h1>Listing dateanswers</h1>

<table>
  <tr>
    <th>Content</th>
    <th>User</th>
    <th>Question</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @dateanswers.each do |dateanswer| %>
  <tr>
    <td><%= dateanswer.content %></td>
    <td><%= dateanswer.user_id %></td>
    <td><%= dateanswer.question_id %></td>
    <td><%= link_to 'Show', dateanswer %></td>
    <td><%= link_to 'Edit', edit_dateanswer_path(dateanswer) %></td>
    <td><%= link_to 'Destroy', dateanswer, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Dateanswer', new_dateanswer_path %>
