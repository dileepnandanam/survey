<div>
<%= form_for(:session, url: sessions_path) do |f| %>


  <div>
    <%= f.label :email %><br />
    <%= f.text_field :email %>
  </div>
  <div>
    <%= f.label :password %><br />
    <%= f.text_field :password %>
  </div>

  <div class="actions" >
    
    <%= f.submit("sign in") %>
  </div>
<% end %>
<%= link_to 'back' , root_path %>
</div>
